{
"manifest_version": 2,
    "name": "Political Correction",
    "description": "Alters text to make articles about politicians a bit more accurate",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}